{
  "name": "gestion-stock-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:local": "npm run gs-api && ng serve -- --proxyConfig=proxy.conf.json",
    "gs-api": "npm run gs-api:create-dist && npm run gs-api:download && npm run gs-api-extract && npm run gs-api-swagger-gen",
    "gs-api:create-dist": "mkdir -p tools/swagger/dist && mkdir -p tool/swagger/src",
    "gs-api:download": "cp E:/Me/Formations/Sources/Gestion de stock/Gestion-stock-backend/target/GestionStock-0.0.1-SNAPSHOT.jar ./tools/swagger/dist && cp E:/Me/Formations/Sources/Gestion de stock/Gestion-stock-backend/target/swagger.json ./tools/swagger/src",
    "gs-api-extract": "node ./tools/swagger/src/swagger.json 0.0.1-SNAPSHOT",
    "gs-api-swagger-gen": "./node_modules/.bin/ng-swagger-gen -i ./tools/swagger/src/swagger.json -o src/gs-api/src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "bootstrap": "^5.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-swagger-gen": "^1.7.0-b",
    "ng-packager": "^1.0.0",
    "typescript": "~4.7.2"
  }
}
